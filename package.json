{
  "name": "syndicate-vault",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "backup": "tsx scripts/backup-database.ts",
    "build": "next build",
    "db:migrate": "chmod +x scripts/apply-migrations.sh && ./scripts/apply-migrations.sh",
    "db:seed": "node scripts/seed-data.js",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "metrics": "curl -f http://localhost:3000/api/metrics || exit 1",
    "migrate": "tsx scripts/run-migrations.ts",
    "prepare": "husky install",
    "restore": "tsx scripts/restore-database.ts",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "validate:env": "tsx scripts/validate-environment.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@jest/globals": "latest",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@upstash/ratelimit": "latest",
    "@upstash/redis": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto": "latest",
    "date-fns": "latest",
    "fs": "latest",
    "isomorphic-dompurify": "latest",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "next-themes": "latest",
    "node-notifier": "latest",
    "path": "latest",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "latest",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "playwright": "^1.40.1",
    "postcss": "^8.5",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.6.2",
    "typescript": "^5"
  }
}